{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run smui-theme && npm run smui-theme:dark && vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync && npm run smui-theme && npm run smui-theme:dark",
		"smui-theme": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme:dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},

	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"sass": "^1.91.0",
		"smui-theme": "^8.0.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"svelte-preprocess": "^6.0.3",
		"typescript": "^5.0.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"@smui/button": "^8.0.3",
		"@smui/card": "^8.0.3",
		"@smui/checkbox": "^8.0.3",
		"@smui/dialog": "^8.0.3",
		"@smui/form-field": "^8.0.3",
		"@smui/icon-button": "^8.0.3",
		"@smui/list": "^8.0.3",
		"@smui/menu": "^8.0.3",
		"@smui/select": "^8.0.3",
		"@smui/snackbar": "^8.0.3",
		"@smui/switch": "^8.0.3",
		"@smui/tab": "^8.0.3",
		"@smui/tab-bar": "^8.0.3",
		"@smui/textfield": "^8.0.3",
		"@smui/top-app-bar": "^8.0.3"
	}
}
